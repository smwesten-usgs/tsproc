END GET_MUL_SERIES_SSF

#==> Reduce OBSERVED time-series data for BLACK EARTH CREEK AT BLACK EARTH (05406500)
START REDUCE_TIME_SPAN
  CONTEXT all
  SERIES_NAME Oq6500i
  NEW_SERIES_NAME Oq6500r
  DATE_1 10/01/1990
  DATE_2 09/30/2000
END REDUCE_TIME_SPAN

#==> Clean OBSERVED time-series data for BLACK EARTH CREEK AT BLACK EARTH (05406500)
START SERIES_CLEAN
  CONTEXT all
  SERIES_NAME Oq6500r
  NEW_SERIES_NAME Oq6500
  SUBSTITUTE_VALUE delete
  UPPER_ERASE_BOUNDARY 0.1
END SERIES_CLEAN

#==> Calc new time base: SIMULATED time-series data for BLACK EARTH CREEK AT BLACK EARTH (05406500)
START NEW_TIME_BASE
  CONTEXT all
  SERIES_NAME Sq6500i
  TB_SERIES_NAME Oq6500
  NEW_SERIES_NAME Sq6500
END NEW_TIME_BASE

#==> DIGITAL FILTER: OBSERVED time-series data for BLACK EARTH CREEK AT BLACK EARTH (05406500)
START DIGITAL_FILTER
  CONTEXT all
  FILTER_TYPE baseflow_separation
  SERIES_NAME Oq6500
  NEW_SERIES_NAME FOq6500
  ALPHA 0.925
  PASSES 3
  CLIP_INPUT yes
  CLIP_ZERO yes
END DIGITAL_FILTER

#==> SERIES EQUATION: SIMULATED time-series data for BLACK EARTH CREEK AT BLACK EARTH (05406500)
START SERIES_EQUATION
  CONTEXT all
  NEW_SERIES_NAME BOq6500
  EQUATION  Oq6500 - FOq6500
END SERIES_EQUATION

#==> DIGITAL FILTER: SIMULATED time-series data for BLACK EARTH CREEK AT BLACK EARTH (05406500)
START DIGITAL_FILTER
